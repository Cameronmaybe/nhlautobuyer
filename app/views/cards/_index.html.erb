<p>Found <%= cards.length %> results </p>
<table class="table table-condensed">
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>POS</th>
    <th>Style</th>
    <th>Actions</th>
  </tr>
  <% cards.each do |card| %>
    <tr >
      <td><%= card.name %></td>
      <td><%= card.player_type.name %></td>
      <td><%= card.position.name %></td>
      <td><%= card.style.name %></td>
      <td>
        <p>-</p>
      </td>
    </tr>

  <% end %>
</table>

